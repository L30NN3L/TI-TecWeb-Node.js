<!-- Declaracao do tipo de documento HTML5 -->
<!DOCTYPE html>
<!-- A propriedade Lang define a linguagem -->
<html lang="pt">
	<head>
	
		<title>NOME DO SISTEMA</title>
	
		<meta charset="utf-8"> 		
		<!-- Para garantir a renderização adequada -->
		<meta name="viewport" content="width=device-width, initial-scale=1">
		
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
		<link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
		<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
		<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
		<script type="text/javascript" src="customizado.js"></script>
		<script type="text/javascript" src="jquery.maskedinput.min.js"></script>
		
	</head>
	
	<body>
	
		<div class="jumbotron text-center">
			<h1>BANNER CADASTRO</h1>
		  <!-- <img src="taq.jpg" class="img-thumbnail" alt="Cinque Terre" width="771" height="250"> -->
		</div>
		
		
			
			<div class="container">
				<fieldset>
					<h1 align="center" >CADASTRO DE VOLUNTÁRIO</h1>
					<form action="/cadastroVoluntario" method="post">
						<div class="panel-body">
							<br>
							<legend>Dados Pessoais - Voluntário</legend>
							<div class="row">

								<div class="linha" name="linha">

								<div class="col-md-6">
									<label for="nomeVoluntario">Nome completo</label>
									<input type="text" class="form-control" id="nomeVoluntario" name="nomeVoluntario">
								</div>
								<div class="col-md-4">
									<label for="dtNascimento">Data de nascimento</label>
									<input type="text" class="form-control calendario" id="dtNascimento" readonly="readonly" name="dtNascimento">
								</div>
								<div class="col-md-2">
									<label for="radioSexo">Sexo</label>
									<div>
										<label class="radio-inline"><input type="radio" name="radioSexo" value="F">Feminino</label>
									</div>
									<div>
										<label class="radio-inline"><input type="radio" name="radioSexo" value="M">Masculino</label>
									</div>
								</div>

							</div>

							</div>
							<div class="row">
								<div class="col-md-4">
									<label for="estadoCivil">Estado Civil</label>
									<select class="form-control" id="estadoCivil" name="estadoCivil">
										<option value="">Selecione</option>
										<option value="solteiro">Solteiro(a)</option>
										<option value="casado">Casado(a)</option>
										<option value="divorciado">Divorciado(a)</option>
										<option value="viuvo">Viúvo(a)</option>
										<option value="separado">Separado(a)</option>
									</select>
								</div>								
								
								<div class="col-md-4">
									<label for="escolaridade">Escolaridade</label>
									<select class="form-control" id="escolaridade" name="escolaridade">
										<option value="">Selecione</option>
										<option value="analfabeto">Analfabeto(a)</option>
										<option value="fundamentalInomp">Fundamental - Incompleto</option>
										<option value="fundamentoalComp">Fundamental - Completo</option>
										<option value="medioIncomp">Médio - Incompleto</option>
										<option value="medioComp">Médio - Completo</option>
										<option value="superiorIncomp">Superior - Incompleto</option>
										<option value="superiorComp">Superior - Completo</option>
										<option value="posIncomp">Pós-graduação - Incompleto</option>
										<option value="posComp">Pós-graduação - Completo</option>
										<option value="mestradoIncomp">Mestrado - Incompleto</option>
										<option value="mestradoComp">Mestrado - Completo</option>
										<option value="doutoradoIncomp">Doutorado - Incompleto</option>
										<option value="doutoradoComp">Doutorado - Completo</option>
									</select>
								</div>
								
								<div class="col-md-4">
									<label for="profissao">Profissão</label>
									<input type="text" class="form-control" id="profissao" name="profissao">
								</div>
							</div>
							
							<div class="row">
																
								<div class="col-md-4">
									<label for="telefone1">Telefone 1</label>
									<input type="text" class="form-control" id="telefone1" name="telefone1">
								</div>
								
								<div class="col-md-4">
									<label for="telefone2">Telefone 2</label>
									<input type="text" class="form-control" id="telefone2" name="telefone2">
								</div>
									
								<div class="col-md-4 cpf">
									<label for="cpf">CPF</label>
									<input type="text" class="form-control" id="cpf" name="cpf">
								</div>
								
								<div class="col-md-4">
									<label for="email">Endereço de Email</label>
									<input type="text" class="form-control" id="email" name="email">
								</div>
								
								<div class="col-md-4">
									<label for="areaAtuacao">Área de interesse em atuar</label>
									<select class="form-control" id="areaAtuacao" name="areaAtuacao">
										<option value="">Selecione</option>         
											   	<option value="Animais">ONG de Animais</option>        
												
												<option value="Apoio a Mulheres">ONG de Apoio a Mulheres</option>        
												
												<option value="Arte">ONG de Arte</option>        
												
												<option value="Crianças">ONG de Crianças</option>        
												
												<option value="Cultura">ONG de Cultura</option>        
												
												<option value="Dependentes Químicos">ONG de Dependentes Químicos</option>        
												
												<option value="Direitos Humanos">ONG de Direitos Humanos</option>        
												
												<option value="Doação de Órgãos">ONG de Doação de Órgãos</option>        
												
												<option value="Educação">ONG de Educação</option>        
												
												<option value="Educação Ambiental">ONG de Educação Ambiental</option>        
												
												<option value="Finanças">ONG de Finanças</option>        
												
												<option value="Geração de Renda">ONG de Geração de Renda</option>        
												
												<option value="Idosos">ONG de Idosos</option>        
												
												<option value="Medicamentos">ONG de Medicamentos</option>        
												
												<option value="Meio Ambiente">ONG de Meio Ambiente</option>        
												
												<option value="Moradia">ONG de Moradia</option>        
																							
												<option value="Reciclagem">ONG de Reciclagem</option>        
												
												<option value="Saúde">ONG de Saúde</option>        
												
												<option value="Situações de Emergência">ONG de Situações de Emergência</option>        
												
												<option value="Tecnologia">ONG de Tecnologia</option>        
												
												<option value="Trabalho">ONG de Trabalho</option>        
												
												<option value="Voluntariado">ONG de Voluntariado Diversos</option>        
									</select>
								</div>
							</div>
							
							<br>
							<legend>Endereço</legend>
							<div class="row">
								<div class="col-md-6">
									<label for="logradouro">Logradouro</label>
									<input type="text" class="form-control" id="logradouro" name="logradouro">
								</div>
								<div class="col-md-2">
									<label for="numero">Número</label>
									<input type="text" class="form-control" id="numero" name="numero">
								</div>
								<div class="col-md-4">
									<label for="complemento">Complemento</label>
									<input type="text" class="form-control" id="complemento" name="complemento">
								</div>
							</div>
							
							<div class="row">
								<div class="col-md-3">
									<label for="bairro">Bairro</label>
									<input type="text" class="form-control" id="bairro" name="bairro">
								</div>
								<div class="col-md-3">
									<label for="cidade">Cidade</label>
									<input type="text" class="form-control" id="cidade" name="cidade">
								</div>
								<div class="col-md-3">
									<label for="estado">UF</label>
									<input type="text" class="form-control" id="estado" name="estado">
								</div>
								<div class="col-md-3">
									<label for="cep">CEP</label>
									<input type="text" class="form-control" id="cep" name="cep">
								</div>
							</div>



							<div class="btn-group" align="left">
				  				<input type="submit" class="btn btn-default" value="Salvar"/>
				 				 <a href="/" class="btn btn-default" role="button">Voltar</a>
							</div>	

					</form>
				</fieldset>
							
			</div>	
	</body>
</html>